namespace com.riiablo.net.packet.d2gs;

union SyncData {
  ClassP,
  CofComponentsP,
  CofTransformsP,
  CofAlphasP,
  PositionP,
  VelocityP,
  AngleP,
  PlayerP,
  DS1ObjectWrapperP,
}

table Sync {
  entityId:int32;
  data:[SyncData];
}

table ClassP {
  type:uint8;
}

table CofComponentsP {
  component:[uint8];
}

table CofTransformsP {
  transform:[uint8];
}

table CofAlphasP {
  alpha:[float32];
}

table PositionP {
  x:float32;
  y:float32;
}

table VelocityP {
  x:float32;
  y:float32;
}

table AngleP {
  x:float32;
  y:float32;
}

table PlayerP {
  charClass:uint8;
  charName:string;
}

table DS1ObjectWrapperP {
  act:uint8;
  id:uint8;
}